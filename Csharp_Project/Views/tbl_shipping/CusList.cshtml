<script>
    const addBtn = $(".fa-plus")
    const modal2 = $('#modal-2')
    const modalContainer1 = $('.js-modal-container')
    const closeBtn1 = $(".js-close-buy-ticket")
    $("#mnv").value = products[products.length - 1].id + 1
    addBtn.addEventListener("click", function () {
        modal2.classList.add('open')
    })
    closeBtn1.addEventListener("click", function () {
        modal2.classList.remove('open')
    })
    modalContainer1.addEventListener("click", function (e) {
        e.stopPropagation();
    })
    thembtn.addEventListener("click", function () {
        if ($("#env").validity.patternMismatch) {
            $("#modal-3").classList.add('open')
            $("#thongbao-modal").innerHTML = "Email không hợp lệ"
        }
        else {
            if ($("#snv").validity.patternMismatch) {
                $("#modal-3").classList.add('open')
                $("#thongbao-modal").innerHTML = "SĐT không hợp lệ"
            }
            else {
                if ($("#anv").validity.patternMismatch) {
                    $("#modal-3").classList.add('open')
                    $("#thongbao-modal").innerHTML = "Đường dẫn không hợp lệ"
                }
                else {
                    if ($("#tnv").value.length == 0 || $("#env").value.length == 0 || $("#pnv").value.length == 0 || $("#snv").value.length == 0 || $("#anv").value.length == 0) {
                        $("#thongbao-modal").innerHTML = "Phải nhập đầy đủ thông tin nhân viên"
                        $("#modal-3").classList.add('open')
                    }
                    else {
                        jQuery.ajax({
                            url: '/tbl_shipping/ThemShipping',
                            dataType: "json",
                            type: "POST",
                            data: {
                                shipping_name: $("#tnv").value,
                                shipping_email: $("#env").value,
                                shipping_password: $("#pnv").value,
                                shipping_phone: $("#snv").value,
                                shipping_img: $("#anv").value,
                            },
                            success: function (data) {
                                if (data > 0) {
                                    $("#thongbao-modal").innerHTML = "Thêm nhân viên thành công"
                                    document.getElementById("thongbao-btn").style.visibility = 'hidden';
                                    $("#modal-3").classList.add('open')
                                    setTimeout(function () {
                                        location.reload()
                                        $("#modal-3").classList.remove('open')
                                        $("#tnv").value = ""
                                        $("#env").value = ""
                                        $("#pnv").value = ""
                                        $("#snv").value = ""
                                        $("#anv").value = ""
                                    }, 1000);

                                }
                                else if (data == 0) {
                                    $("#thongbao-modal").innerHTML = "Thêm nhân viên thất bại"
                                    $("#modal-3").classList.add('open')
                                }
                                else {
                                    $("#thongbao-modal").innerHTML = "Email đã sử dụng, vui lòng dùng email khác"
                                    $("#modal-3").classList.add('open')
                                }
                            },
                            error: function (xhr) {
                                $("#thongbao-modal").innerHTML = "Xảy ra lỗi"
                                $("#modal-3").classList.add('open')
                            }
                        });
                    }
                }
            }
        }
    })
</script>
