            } else {
                i.nextElementSibling.childNodes[1].lastElementChild.firstChild.classList.remove("fa-lock")
                i.nextElementSibling.childNodes[1].lastElementChild.firstChild.classList.add("fa-unlock")
            }
        }
        for (var i of blockBtn) {
            i.addEventListener("click", function (e) {
                var th = this
                e.preventDefault()
                this.offsetParent.parentElement.children[4].children[0].children[0].classList.remove("fa-lock")
